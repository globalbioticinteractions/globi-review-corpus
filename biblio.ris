# export indexed interaction records
elton interactions globalbioticinteractions/mpm\
 > interactions.tsv

# export names and align them with the Catalogue of Life using Nomer 
elton names globalbioticinteractions/mpm\
 | nomer append col\
 > name-alignment.tsv

