# export indexed interaction records
elton interactions globalbioticinteractions/paz2021\
 > interactions.tsv

# export names and align them with the Catalogue of Life using Nomer 
elton names globalbioticinteractions/paz2021\
 | nomer append col\
 > name-alignment.tsv

